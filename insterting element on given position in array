#include<iostream>
using namespace std;
int main (){
	
	int arr[5]={1,2,3,4,5};
	for(int i=0;i<5; i++){
		cout<<arr[i]<<endl;
	}
	int index;
	cout<<"enter index number ";
	cin>>index;
	
	 int num;
	cout<<"number";
	cin>>num;
	
	for (int i=0;i<5; i++){
		if(arr[index] = num){
	     cout<<arr[i];
        }
	}
	
	
    return 0;

}